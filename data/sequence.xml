<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_job_request" model="ir.sequence">
            <field name="name">Job Request Sequence</field>
            <field name="code">yacht.repair</field>
            <field name="prefix">JR-%(y)s-</field>
            <field name="padding">3</field>
        </record>

        <record model="ir.sequence" id="sequence_sale_order">
            <field name="name">Work Order Sequence</field>
            <field name="code">sale.order.sequence</field>
            <field name="prefix">WO-%(y)s-</field> <!-- Change the prefix to 'WO' -->
            <field name="padding">3</field> <!-- You can adjust the padding to match your desired format -->
        </record>
    </data>
</odoo>